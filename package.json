{
  "name": "vibefolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev --turbo",
    "dev:clean": "rm -rf .next && npm run dev",
    "build": "rm -rf .next && next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint",
    "generate:transactions": "tsx --tsconfig ./tsconfig.scripts.json ./src/data/generateData.ts",
    "db:staging:reset": "supabase db reset --db-url $STAGING_DATABASE_URL",
    "db:prod:reset": "supabase db reset --db-url $DATABASE_URL",
    "db:staging:push": "supabase db push --db-url $STAGING_DATABASE_URL",
    "db:prod:push": "supabase db push --db-url $DATABASE_URL",
    "db:staging:seed": "psql $STAGING_DATABASE_URL -f combined_data.sql",
    "db:prod:seed": "psql $DATABASE_URL -f combined_data.sql"
  },
  "dependencies": {
