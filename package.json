    "date-fns": "^3.6.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.20.1",
    "geist": "^1.3.1",
    "lucide-react": "^0.544.0",
    "next": "^15.2.4",
    "next-themes": "^0.4.4",
    "nuqs": "^2.3.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-intersection-observer": "^9.16.0",
    "recharts": "^2.15.0",
    "sonner": "^2.0.7",
